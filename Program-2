import java.util.*;
public class Areaofcircle
{
	public static void main(String argus[])
	{
		double r,area;
		final double Pi=3.14;
                System.out.println("Krinshsmith Kava");
                System.out.println("Er no. 220130318102");
		Scanner sc=new Scanner(System.in);
